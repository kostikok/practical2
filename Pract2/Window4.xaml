<Window x:Class="Pract2.Window4"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pract2"
        mc:Ignorable="d"
        Title="Window4" Height="450" Width="800">
    <Grid>
        <Button x:Name="returnbutton" Content="Вернуться" HorizontalAlignment="Left" Margin="10,374,0,0" VerticalAlignment="Top" Height="35" Width="65" Background="Red" Click="returnbutton_Click"/>
        <PasswordBox x:Name="Password" HorizontalAlignment="Center" Margin="0,184,0,0" VerticalAlignment="Top" Width="130" Height="33"/>
        <Label Content="Введите пароль" HorizontalAlignment="Center" Margin="0,139,0,0" VerticalAlignment="Top" Width="170" Height="40" FontSize="20" FontWeight="Bold"/>
        <Button x:Name="Enter" Content="Ввод" HorizontalAlignment="Center" Margin="0,222,0,0" VerticalAlignment="Top" Height="25" Width="50" Background="#FF66F1FB" Click="Enter_Click"/>
        <Label x:Name="Secret" Content="" HorizontalAlignment="Left" Margin="465,188,0,0" VerticalAlignment="Top" Width="290" FontWeight="Bold" Height="29" FontSize="11"/>

    </Grid>
</Window>
